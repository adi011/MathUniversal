<Page
    x:Class="MathUniversal.TheSystemOfEquationsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MathUniversal"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    DataContext="{Binding Source={StaticResource Locator}, Path=TheSystemOfEquations}"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <TextBox Margin="10,10,10,557" Background="#999"  />
        <TextBox Margin="10,88,10,469" Background="#999"/>
        <Button Content="TemporaryBackNavigate" Command="{Binding SystemOfEquations}"/>

    </Grid>
</Page>
