<Page
    x:Class="MathUniversal.TheSystemOfEquationsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MathUniversal"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    DataContext="{Binding Source={StaticResource Locator}, Path=TheSystemOfEquations}"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Grid Background="#3F53ff">
            <TextBox Margin="10,32,103,587" Foreground="#8B97FF" Background="#464C7F"  />
            <TextBox Margin="10,88,103,530" Foreground="#8B97FF" Background="#464C7F"/>
            <Button Content="TemporaryBackNavigate" Foreground="#8B97FF" Background="#464C7F" Command="{Binding BackToStartCommand}"/>
        </Grid>
    </Grid>
</Page>
